# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# HubSpot Integration (via n8n webhook)
NEXT_PUBLIC_N8N_WEBHOOK_URL=your_n8n_webhook_url_here

# Email Service (MailerLite/MailerSend)
MAILERLITE_API_KEY=your_mailerlite_api_key_here
MAILERSEND_API_KEY=your_mailersend_api_key_here

# Choose your email provider (mailerlite or mailersend)
EMAIL_PROVIDER=mailerlite

# Email Templates (template IDs from your email service)
LEVEL_AI_TEMPLATE_ID=your_level_ai_template_id
TECH4GOOD_TEMPLATE_ID=your_tech4good_template_id
RAISE_TEMPLATE_ID=your_raise_template_id

# Optional: Default theme (levelai, tech4good, or raise)
NEXT_PUBLIC_DEFAULT_THEME=levelai